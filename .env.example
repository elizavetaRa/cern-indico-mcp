# Indico MCP Server Configuration
# This server only accesses PUBLIC events (no authentication)

# Base URL for Indico instance
INDICO_BASE_URL=https://indico.cern.ch

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Enable caching for better performance (true/false)
ENABLE_CACHE=true

# Cache size (number of cached requests)
CACHE_SIZE=128
